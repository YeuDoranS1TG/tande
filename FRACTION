#include<bits/stdc++.h>
using namespace std;
#define maxN 1000000
int d[maxN+5]={0};
void factorial(int res, map<int,int> &mp, int x){
    while (res>1){
        int p=d[res];
        if (p==0) p=res;
        int cnt=0;
        while (res%p==0){
            ++cnt;
            res/=p;
        }
        mp[p]+=x*cnt;
    }
}
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    freopen("FRACTION.INP","r",stdin);
    freopen("FRACTION.OUT","w",stdout);
    int T;
    cin>>T;
    for (int a=2;a*a<=maxN;++a){
        if (d[a]==0){
            for (int b=a;a*b<=maxN;++b){
                d[a*b]=a;
            }
        }
    }
    while (T--){
        int n; cin>>n;
        map<int,int>a;
        for (int i=0;i<n;++i){
            int res;
            cin>>res;
            factorial(res,a,1);
        }
        for (int i=0;i<n;++i){
            int res;
            cin>>res;
            factorial(res,a,-1);
        }
        bool ok=false;
        for (auto &v:a){
            if (v.second<0 && v.first!=2 && v.first!=5){
                ok=true;
            }
        }
        if (ok) cout<<"repeating"<<'\n';
        else cout<<"finite"<<'\n';
    }
    return 0;
}
