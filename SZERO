#include <bits/stdc++.h>
using namespace std;
#define maxN 100005
long long n,a[maxN],resi,resj;
int main()
{
    //freopen("SZERO.INP","r",stdin);
    //freopen("SZERO.OUT","w",stdout);
    cin>>n;
    for (int i=0;i<n;++i){
        cin>>a[i];
    }
    vector<pair<long long,long long>>v(n);
    long long prefixsum=0;
    for (int i=0;i<n;++i){
        prefixsum+=a[i];
        v[i].first=prefixsum;
        v[i].second=i;
    }
    long long maxval=0;
    sort(v.begin(),v.end());
    for (int i=0;i<n-1;++i){
        int j=i+1;
        while (j<n && v[i].first==v[j].first){
            if (v[j].second-v[i].second>maxval){
                maxval=v[j].second-v[i].second;
                resi=v[i].second;
                resj=v[j].second;
            }
            ++j;
        }
    }
    cout<<resi+2<<" "<<resj+1;
    return 0;
}
