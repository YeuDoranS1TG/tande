#include <bits/stdc++.h>
using namespace std;
vector<int>v,a;
int n,cnt=0;
void bam_nut_mot(){
    for (int i=0;i<n;++i){
        v[i]=(v[i]+1)%10;
    }
}
string chuyenso(){
    string val;
    for (int i=0;i<n;++i){
        val.push_back(char('0'+a[i]));
    }
    return val;
}
void bam_nut_hai(){
    int res=a.back();
    for (int i=n-1;i>0;--i){
        a[i]=a[i-1];
    }
    a[0]=res;
}
int main()
{
    freopen("LOCK.INP","r",stdin);
    freopen("LOCK.OUT","w",stdout);
    cin>>n;
    string s; cin>>s;
    for (int i=0;i<n;++i){
        int x=s[i]-'0';
        v.push_back(x);
    }
    string minval(n,'9');
    for (int solan=0;solan<10;++solan){
        a=v;
        for (int i=0;i<n;++i){
            string num=chuyenso();
            if (num<minval){
                minval=num;
            }
            bam_nut_hai();
        }
        bam_nut_mot();
    }
    cout<<minval;
    return 0;
}
