#include <bits/stdc++.h>
using namespace std;
int n;
vector<long long>a;
int main()
{
    freopen("BIRTHCAKES.INP","r",stdin);
    freopen("BIRTHCAKES.OUT","w",stdout);
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cin>>n;
    for (int i=0;i<n;++i){
        long long x;
        cin>>x;
        a.push_back(x);
    }
    sort(a.begin(),a.end());
    int cnt=n;
    int i=0,j=n/2;
    while (i<n/2 && j<n){
        if (a[i]*2<=a[j]){
            --cnt;
            ++i,++j;
        }
        else {
            ++j;
        }
    }
    cout<<cnt;
    return 0;
}
