#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    freopen("TOWERS.INP","r",stdin);
    freopen("TOWERS.OUT","w",stdout);
    vector<long long>a;
    int n;
    vector<int>v;
    cin>>n;
    for (int i=0;i<n;++i){
        long long x;
        cin>>x;
        a.push_back(x);
    }
    v.push_back(a[0]);
    long long cnt=1;
    for (int i=1;i<n;++i){
        auto it=upper_bound(v.begin(),v.end(),a[i]);
        if (it==v.end()){
            v.push_back(a[i]);
            ++cnt;
        }
        else {
            *it=a[i];
        }
    }
    cout<<cnt;
    return 0;
}
