#include <bits/stdc++.h>
using namespace std;
#define maxN 100005
int n;
long long a[maxN];
int main()
{
    //freopen("PS.INP","r",stdin);
    //freopen("PS.OUT","w",stdout);
    cin>>n;
    for (int i=0;i<n;++i) cin>>a[i];
    vector<pair<long long,long long>>v(n);
    long long prefixsum=0;
    for (int i=0;i<n;++i){
        prefixsum+=a[i];
        v[i].first=prefixsum;
        v[i].second=i+1;
    }
    sort(v.begin(),v.end());
    long long maxval=0,resi,resj,minId=v[0].second;
    for (int i=1;i<n;++i){
        if (v[i].second>minId && v[i].first!=v[i-1].first){
            int distance=v[i].second-minId;
            if (distance>maxval){
                maxval=distance;
                resi=v[i].second;
                resj=minId;
            }
        }
        minId=min(minId,v[i].second);
    }
    cout<<resj+2<<" "<<resi+1;
    return 0;
}
/*
ĐOẠN DƯƠNG 
Cho dãy số nguyên 𝐴 = (𝑎1,𝑎2,…,𝑎𝑛)(|𝑎𝑖| ≤ 109). Hãy tìm một đoạn dài nhất gồm các phần tử liên tiếp 
trong dãy 𝐴: (𝑎𝐿,𝑎𝐿+1,…,𝑎𝐻) có tổng là số dương. 
Dữ liệu: Vào từ tập tin văn bản PS.INP - - 
Dòng 1: Chứa số nguyên dương 𝑛(𝑛 ≤ 105) 
Dòng 2: Chứa 𝑛 số 𝑎1,𝑎2,…,𝑎𝑛 theo đúng thứ tự, đảm bảo có ít nhất một số dương trong dãy. 
Kết quả: Ghi ra tập tin văn bản PS.OUT hai chỉ số 𝐿 và 𝐻 trên một dòng 
Ví dụ: 
PS.INP 
10 -5 -2 -3 4 -6 7 -8 9 -1 -20 
PS.OUT 
3 9
*/
