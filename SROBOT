#include <bits/stdc++.h>
using namespace std;
#define ll long long
pair<ll,ll> direction(char c, long long res1, long long res2){
    if (c=='R'){
        return {res1+1,res2};
    }
    else if (c=='U'){
        return {res1,res2+1};
    }
    else if (c=='D'){
        return {res1,res2-1};
    }
    return {res1-1,res2};
}
long long area(pair<ll,ll>&h, pair<ll,ll>&j,pair<ll,ll>&f){
    long long x1=h.first, y1=h.second;
    long long x2=j.first,y2=j.second;
    long long x3=f.first,y3=f.second;
    return (y2+y1)*(x2-x1)+(y3+y2)*(x3-x2)+(y1+y3)*(x1-x3);
}
int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);
    //freopen("SROBOT.INP","r",stdin);
    //freopen("SROBOT.OUT","w",stdout);
	long long x,y;
    while(cin>>x>>y){
        pair<ll,ll>q={x,y};
        string s;
        cin>>s;
        int n=(int)s.size();
        vector<pair<ll,ll>>v;
        v.push_back({0,0});
        for (int i=0;i<2*n;i++){
            char c = s[i % n];
            long long px=v.back().first, py=v.back().second;
            v.push_back(direction(c, px, py));
        }
        //int m=v.size();
        bool ok=false;
        for (int i=0;i<n;++i){
            if (v[i]==v[i+n]){
                if (v[i]==q){
                    cout<<"YES"<<'\n';
                }
                else cout<<"NO"<<'\n';
                ok=true;
                break;
            }
            long long ar=area(v[i],v[i+n],q);
            if (ar==0){
                //cout<<i<<'\n';
                cout<<"YES"<<'\n';
                ok=true;
                break;
            }
        }
        if (!ok) cout<<"NO"<<'\n';
    }
}
