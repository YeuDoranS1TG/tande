#include <bits/stdc++.h>
using namespace std;
long long n,k;
vector<long long>s,h;
long long binarysearch(long long val){
    long long res=lower_bound(h.begin(),h.end(),val)-h.begin();
    if (res>n) return 0;
    long long cnt=n-res;
    if (res>0) return s[n-1] - s[res-1] - cnt*val;
    else return s[n-1] - cnt*val;
}
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    freopen("SAW.INP","r",stdin);
    freopen("SAW.OUT","w",stdout);
    cin>>n>>k;
    h.resize(n);
    long long prefixsum=0;
    for (long long i=0;i<n;++i) cin>>h[i];
    sort(h.begin(),h.end());
    s.resize(n);
    s[0]=h[0];
    for (long long i=1;i<n;++i) s[i]=s[i-1] + h[i];
    while (k--){
        long long m;
        cin>>m;
        long long ans=0;
        long long lo=0, hi=h.back();
        while (lo<=hi){
            long long mid=(lo+hi)/2;
            long long pos=binarysearch(mid);
            if (pos>=m){
                ans=mid;
                lo=mid+1;
            }
            else hi=mid-1;
        }
        cout<<ans<<" ";
    }
    return 0;
}
