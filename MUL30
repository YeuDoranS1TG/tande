#include <bits/stdc++.h>
using namespace std;

bool ss(int x, int y){
    return x>y;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    freopen("MUL30.INP","r",stdin);
    freopen("MUL30.OUT","w",stdout);
    bool ok=false;
    string s;
    cin>>s;
    int n=s.size();
    int sum=0;
    vector<int>v;
    for (int i=0;i<n;++i){
        if (s[i]=='0') ok=true;
        sum+=(s[i]-'0');
        v.push_back((s[i]-'0'));
    }
    if (!ok || sum%3!=0){
        cout<<-1;
        return 0;
    }
    else {
        sort(v.begin(),v.end(),ss);
        for (auto a:v){
            cout<<a;
        }
    }
    return 0;
}
