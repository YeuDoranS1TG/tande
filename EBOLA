#include<bits/stdc++.h>
using namespace std;
#define maxN 100000
vector<int>v[maxN+5];
bool visited[maxN+5];
vector<int>a;
int n,k;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);
    freopen("EBOLA.INP","r",stdin);
    freopen("EBOLA.OUT","w",stdout);
    cin>>n>>k;
    for (int i=1;i<=n;++i){
        int m;
        cin>>m;
        while (m--){
            int x;
            cin>>x;
            v[i].push_back(x);
            v[x].push_back(i);
        }
    }
    memset(visited,false,sizeof(visited));
    //int cnt=1;
    visited[k]=true;
    a.push_back(k);
    queue<int>q;
    q.push(k);
    while (!q.empty()){
        int u=q.front();
        q.pop();
        for (auto &x:v[u]){
            if (!visited[x]){
                a.push_back(x);
                visited[x]=true;
                q.push(x);
            }
        }
    }
    cout<<a.size()<<'\n';
    sort(a.begin(),a.end());
    for (auto &x:a) cout<<x<<" ";
    return 0;
}
